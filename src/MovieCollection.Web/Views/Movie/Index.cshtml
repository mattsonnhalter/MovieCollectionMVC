@model IList<MovieCollection.Web.Models.Movie>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    ViewBag.Title = "Index";
}

<ul>
    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <li>@item.Id</li>
            <li>@item.Title</li>
            <li>@item.ReleaseDate</li>
            <li>@item.ImageUrl</li>
        }
    }
</ul>